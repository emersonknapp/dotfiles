"*":
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  core:
    disabledPackages: [
      "markdown-preview"
      "you-complete-me"
      "autocomplete"
      "autocomplete-clang"
    ]
    telemetryConsent: "no"
  editor:
    fontSize: 11
    preferredLineLength: 100
    showIndentGuide: true
  "exception-reporting":
