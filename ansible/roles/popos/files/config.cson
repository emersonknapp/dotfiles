  "git-blame":
    columnWidth: 257
  "linter-flake8":
    projectConfigFile: [
      "/home/emerson/dev/ros/master/src/ament/ament_lint/ament_flake8/ament_flake8/configuration/ament_flake8.ini"
    ]
  "linter-ui-default":
    panelHeight: 300
  "split-diff": {}
  "vim-mode-plus":
    ignoreCaseForSearch: true
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
